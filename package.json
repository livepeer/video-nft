{
	"name": "@livepeer/video-nft",
	"version": "0.3.6",
	"description": "1-command minting of video NFTs",
	"keywords": [
		"video",
		"nft",
		"video-nft",
		"livepeer",
		"ipfs"
	],
	"author": {
		"name": "Livepeer",
		"email": "infraservice@livepeer.org",
		"url": "https://livepeer.com/"
	},
	"homepage": "https://livepeer.com/dashboard/assets",
	"license": "MIT",
	"main": "dist/index.cjs.js",
	"module": "dist/index.esm.js",
	"browser": "dist/index.umd.js",
	"cli": "dist/cli.js",
	"types": "dist/index.d.ts",
	"bin": "bin/video-nft",
	"directories": {
		"src:": "src",
		"bin:": "bin",
		"dist": "dist"
	},
	"files": [
		"src",
		"bin",
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/livepeer/video-nft.git"
	},
	"scripts": {
		"prepare": "yarn clean && yarn format && yarn build",
		"build": "yarn build:rollup && yarn build:docs",
		"build:rollup": "rollup -c",
		"build:docs": "typedoc",
		"dev": "rollup -c -w",
		"dev:docs": "watch-http-server ./dist/docs & typedoc --watch --preserveWatchOutput",
		"start": "yarn build && ./bin/video-nft",
		"format": "prettier --write \"./**/*.{ts,js,json}\"",
		"clean": "rm -rf ./dist"
	},
	"bugs": {
		"url": "https://github.com/livepeer/video-nft/issues"
	},
	"dependencies": {
		"axios": "^0.26.0",
		"browser-fs-access": "^0.25.0",
		"ethers": "^5.6.1",
		"inquirer": "^8.2.0",
		"progress-stream": "^2.0.0",
		"yargs": "^17.3.1"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^21.0.2",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@types/axios": "^0.14.0",
		"@types/inquirer": "^8.2.0",
		"@types/node": "^17.0.18",
		"@types/progress-stream": "^2.0.2",
		"@types/rollup": "^0.54.0",
		"@types/yargs": "^17.0.8",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"esbuild": "^0.14.27",
		"eslint": "^8.9.0",
		"prettier": "^2.5.1",
		"rollup": "^2.70.1",
		"rollup-plugin-babel-minify": "^10.0.0",
		"rollup-plugin-dts": "^4.2.0",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-typescript2": "^0.31.2",
		"ts-node": "^7.0.1",
		"tslib": "^1.9.3",
		"typedoc": "^0.22.13",
		"typescript": "^4.6.3",
		"watch-http-server": "^0.7.6"
	},
	"optionalDependencies": {
		"fsevents": "^1.2.13"
	}
}
